{
  "name": "enhanceai",
  "version": "0.3.1",
  "description": "React wrapper for EnhanceAI (enhanceai.dev), a library for enhancing web app text input with AI autocomplete.",
  "author": "Walden Yan (waldenyan20@gmail.com)",
  "keywords": [
    "react",
    "components",
    "ai",
    "ui",
    "enhanceai"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walnutwaldo/enhanceai-react.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": ">=7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/node": "^16.18.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "core-js": ">=3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "build:react": "NODE_ENV=production babel src/lib --extensions \".ts,.tsx\" --out-dir dist --copy-files",
    "build:dts": "tsc",
    "build": "rm -rf dist && npm run build:react && npm run build:dts"
  },
  "license": "MIT"
}
